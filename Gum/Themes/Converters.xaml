<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="clr-namespace:Gum.Converters">
    <converters:NullToVisibilityConverter x:Key="NullToVisibilityConverter" />
    <converters:NullToVisibilityConverter x:Key="InverseNullToVisibilityConverter" Invert="True" />
    <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    <converters:BoolToVisibilityConverter x:Key="InverseBoolToVisibilityConverter" Invert="True" />
    <converters:NullToBoolConverter x:Key="NullToBoolConverter" />
    <converters:NullToBoolConverter x:Key="InverseNullToBoolConverter" Invert="True" />
</ResourceDictionary>